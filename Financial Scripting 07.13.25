import pandas as pd  #USING BASE CONDA to run yfinance, This is for Yearly Financials
import yfinance as yf
def importing_yf_financials_into_folder(ticker):
    ticker = input()
    Ticker2 = yf.Ticker(ticker)
    pnl =Ticker2.financials
    bs = Ticker2.balancesheet
    fs1 = pd.concat([pnl,bs])
    fs1.to_csv(" " + ticker +'_Financials'+ '.csv')   # Put where you want file to be placed
    return fs1
importing_yf_financials_into_folder('x')


import pandas as pd  #USING BASE CONDA to run yfinance , This is for Quarterly Financials
